// Try Prisma Accelerate: https://pris.ly/cli/accelerate-init

generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native", "darwin-arm64"]
}

datasource db {
  provider = "postgresql"
  url      = DATABASE_URL
}

model newfile {
  id        Int      @id @default(autoincrement())
  title     String
  content   String
  link      String
}
